{
  "name": "web-vue",
  "version": "1.0.1",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "build": "nuxt prepare && nuxt build",
    "start": "node .output/server/index.mjs",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:init": "tsx scripts/init-db.ts",
    "db:seed": "tsx scripts/seed-db.ts",
    "db:seed-packages": "tsx scripts/seed-packages.ts",
    "db:test": "tsx scripts/test-db.ts",
    "db:test-workflows": "tsx scripts/test-workflows.ts",
    "migrate": "tsx scripts/migrate.ts",
    "migrate:up": "tsx scripts/migrate.ts up",
    "migrate:status": "tsx scripts/migrate.ts status",
    "migrate:rollback": "tsx scripts/migrate.ts rollback",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/s3-request-presigner": "^3.709.0",
    "@nuxt/content": "^2.13.4",
    "@nuxt/ui": "^3.0.0",
    "@pinia/nuxt": "^0.11.3",
    "@stripe/stripe-js": "^8.4.0",
    "@trpc/client": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "bcrypt": "^6.0.0",
    "chart.js": "^4.5.1",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-vue-next": "^0.553.0",
    "nodemailer": "^7.0.11",
    "nuxt": "^4.2.1",
    "pg": "^8.16.3",
    "pinia": "^3.0.4",
    "stripe": "^17.4.0",
    "superjson": "^2.2.5",
    "trpc-nuxt": "^1.2.0",
    "vue": "^3.5.24",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.2.3",
    "@nuxt/icon": "^2.1.0",
    "@nuxtjs/tailwindcss": "^5.3.5",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.15.6",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.22",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vitest": "^2.1.8"
  }
